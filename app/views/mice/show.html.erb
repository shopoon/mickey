<div class="page-header">
  <h1><%=t '.title', :default => Mouse.model_name.human.titleize %></h1>
</div>

<div class="fieldset">
  <dl>
    <dt class="label label-default"><%= Family.human_attribute_name(:genotype) %></dt>
    <dd><pre class="prettyprint"><%= @mouse.family.genotype %></pre></dd>
    <dt class="label label-default"><%= Mouse.human_attribute_name(:family_id) %></dt>
    <dd><pre class="prettyprint"><%= @mouse.family_id %></pre></dd>
    <dt class="label label-default"><%= Family.human_attribute_name(:birth) %></dt>
    <dd><pre class="prettyprint"><%= @mouse.family.birth %></pre></dd>
    <dt class="label label-default"><%= Mouse.human_attribute_name(:sex) %></dt>
    <dd><pre class="prettyprint"><%= @mouse.sex %></pre></dd>
    <dt class="label label-default"><%= Mouse.human_attribute_name(:sequence_id) %></dt>
    <dd><pre class="prettyprint"><%= @mouse.sequence_id %></pre></dd>
    <dt class="label label-default"><%= Mouse.human_attribute_name(:status) %></dt>
    <dd><pre class="prettyprint"><%= @mouse.status %></pre></dd>
  </dl>
</div>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              mice_path, :class => 'btn btn-default'  %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              mouse_path(@mouse),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
